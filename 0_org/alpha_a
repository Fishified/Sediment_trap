FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha_a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0; 

boundaryField
{
    upstreamInlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    downstreamOutlet
    {
       type            zeroGradient;
    }

    plane
    {
	type		symmetryPlane;
    }

    top
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }

}


